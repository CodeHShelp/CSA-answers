public class RectangleRunner {
    public static void main(String[] args) {
        // Create rectangle objects from class
        
        // 5. Change the color of rect1 to "yellow" as per the instructions.
        Rectangle rect1 = new Rectangle(5, 8, "yellow");
        
        Rectangle rect2 = new Rectangle(2, 10, "blue");
        
        Rectangle rect3 = new Rectangle(10, 3, "violet");
        
        // 6. Create the fourth object, rect4, with new values.
        Rectangle rect4 = new Rectangle(6, 4, "red");

        // Print out rectangle objects (calls toString() method)
        System.out.println(rect1);
        System.out.println(rect2);
        System.out.println(rect3);
        
        // 7. Print the new object, rect4.
        System.out.println(rect4);
    }
}
