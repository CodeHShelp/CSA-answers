public class Houses
{
    public static void main(String[] args) {
    // First house: roof, two levels, and a sidewalk
    buildRoof();
    buildLevel();
    buildLevel();
    buildSidewalk();

    System.out.println(); // Blank line between houses

    // Second house: roof, four levels, and a sidewalk
    buildRoof();
    buildLevel();
    buildLevel();
    buildLevel();
    buildLevel();
    buildSidewalk();
    }

    // Define your methods here
    public static void buildRoof() {
    System.out.println("   /\\");
    System.out.println("  /  \\");
    System.out.println(" /____\\");
    }
    
    public static void buildLevel() {
    System.out.println("|    |");
    System.out.println("|____|");
    }

    public static void buildSidewalk() {
    System.out.println("-");
    System.out.println("-");
    System.out.println("------");
    }
}

